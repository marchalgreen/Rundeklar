{
  "name": "@newco/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "postinstall": "prisma generate || echo \"(prisma generate skipped)\"",
    "build": "pnpm run validate && prisma generate && next build --turbopack",
    "postbuild": "if [ -n \"$CI\" ]; then prisma migrate deploy; fi",
    "start": "next start",
    "lint": "eslint --quiet . ./tests ../../scripts ../../tests",
    "lint:fix": "eslint --fix . ./tests ../../scripts ../../tests",
    "format": "prettier --write . ./tests ../../scripts ../../tests",
    "format:check": "prettier --check . ./tests ../../scripts ../../tests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "validate": "pnpm run typecheck && pnpm run lint",
    "test": "pnpm exec tsx --test",
    "test:e2e": "pnpm exec playwright test --config=../../playwright.config.ts --project=web",
    "seed": "tsx lib/auth/data.seed.ts",
    "sync-env": "bash ../../push-env.sh",
    "scrape:moscot": "node --experimental-strip-types ../../scripts/moscot-scrape.ts",
    "scrape:moscot:quick": "MOSCOT_PAGES=2 MOSCOT_CONCURRENCY=1 pnpm scrape:moscot",
    "sync:moscot": "node --experimental-strip-types ../../scripts/sync-moscot.ts",
    "import:demo": "node --experimental-strip-types ../../scripts/import-demo-catalog.ts"
  },
  "dependencies": {
    "@glideapps/glide-data-grid": "^6.0.3",
    "@glideapps/glide-data-grid-cells": "^6.0.3",
    "@node-rs/argon2": "^2.0.2",
    "@phosphor-icons/react": "^2.1.10",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/render": "^1.3.2",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.22",
    "fuse.js": "^7.1.0",
    "input-otp": "^1.4.2",
    "jose": "^6.1.0",
    "jsbarcode": "^3.12.1",
    "lucide-react": "^0.544.0",
    "nanoid": "^5.1.6",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "otplib": "^12.0.1",
    "p-limit": "^5.0.0",
    "react": "19.1.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.1.0",
    "react-rnd": "^10.5.2",
    "recharts": "^2.15.4",
    "resend": "^6.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tesseract.js": "^4.1.1",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.5.4",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/recharts": "^2.0.1",
    "@types/supertest": "^6.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.0",
    "jscodeshift": "^17.3.0",
    "playwright": "^1.56.1",
    "prisma": "^6.17.1",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "typescript-eslint": "^8.46.0"
  }
}
